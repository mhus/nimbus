# Nimbus Client Configuration
# Copy this file to .env and fill in your values

# Authentication
VITE_CLIENT_USERNAME=testuser
VITE_CLIENT_PASSWORD=testpass

# Server Configuration
VITE_SERVER_WEBSOCKET_URL=ws://localhost:3011
VITE_SERVER_API_URL=http://localhost:3011

# Logging (optional)
VITE_LOG_TO_CONSOLE=true

# Note: Environment variables must be prefixed with VITE_ for Vite to expose them to the browser
